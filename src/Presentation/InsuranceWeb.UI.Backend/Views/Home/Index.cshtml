@model List<CustomModel>

<div>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">CustomerName</th>
                <th scope="col">CustomerSurname</th>
                <th scope="col">InsuredName</th>
                <th scope="col">PolicyName</th>
                <th scope="col">PolicyPrice</th>
                <th scope="col">Prim</th>
                <th scope="col">AmountCount</th>
                <th scope="col">TotalPrice</th>
                <th scope="col">ApprovingName</th>
                <th scope="col">PurchaseOrderStatusName</th>
            </tr>
        </thead>
        @foreach (var item in Model)
        { 
            
        <tbody>
            <tr>
                <th scope="row"></th>
                <td>item.CustomerName</td>
                <td>item.CustomerSurname</td>
                <td></td>
            </tr>
            <tr>
                <th scope="row">2</th>
                <td>Jacob</td>
                <td>Thornton</td>
                <td></td>
            </tr>
            <tr>
                <th scope="row">3</th>
                <td>Larry</td>
                <td>the Bird</td>
                <td></td>
            </tr>
        </tbody>
        }

    </table>
</div>